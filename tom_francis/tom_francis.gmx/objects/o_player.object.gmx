            <string>/*
VECTOR-BASED MOVEMENT
RISE ABOVE 8 IF STATEMENTS
*/

//initial vectors are 0
x_vector = 0;
y_vector = 0;

key_pressed = 0

//each button pressed adds or subtracts from the x or y vector
if (keyboard_check(ord('W')))
{
    y_vector -= 1
    key_pressed = 1
}

if (keyboard_check(ord('S')))
{
    y_vector += 1
    key_pressed = 1
}

if (keyboard_check(ord('A')))
{
    x_vector -= 1
    key_pressed = 1
}

if (keyboard_check(ord('D')))
{
    x_vector += 1
    key_pressed = 1
}


//the direction is the sum of the vectors
direction = point_direction(x, y, x + x_vector, y + y_vector)

//if one of the moevement keys was pressed, the player moves!
if key_pressed = 0
{
    speed = 0
}

else
{
    speed = player_speed
}



//hit detection with enemies
incoming_enemy = instance_place(x,y, o_enemyparent)

if instance_exists(incoming_enemy)
{
    //destroys the player
    instance_destroy()
    
    //destroys the enemy
    with incoming_enemy
        instance_destroy()
}
</string>
